<div class="container" style="display: flex; justify-content: center; color: #E6EBEE; font-family: montserrat; ">
<%= simple_form_for partner do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :gender, collection: Partner::GENDER %>
  <%= f.input :address %>
  <%= f.input :temperament, collection: Partner::TEMPERAMENT %>
  <%= f.input :interest, collection: Partner::INTEREST %>
  <%= f.input :age %>
   <%= f.input :photo_url, as: :file %>
  <%= f.submit "Create/Edit", class:'create-partner-button btn-rent link-index-button btn-appointment' %>
<% end %>
</div>
